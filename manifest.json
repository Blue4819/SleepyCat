{
  "name": "SleepyCat",
  "version": "1.0",
  "description": "Sleep reminders",
  "manifest_version": 3,
  "author": "Summer Wang, Rishika Thandra, Prachi Bharti",
  "action":{
      "default_popup": "popup.html",
      "default_icon": "Cat.png"
  },
  "permissions":[
    "unlimitedStorage",
    "geolocation",
    "alarms",
    "notifications",
    "system.display"
    ]
}
